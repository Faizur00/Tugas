START
    INPUT WAKTU KERJA MINGGUAN

    UPAH POKOK = 2000
    UPAH LEMBUR = 3000
    BATAS JAM NORMAL = 48
    UPAH MINGGUAN = 0

    IF WAKTU KERJA MINGGUAN <= BATAS JAM NORMAL
        UPAH MINGGUAN = WAKTU KERJA MINGGUAN * UPAH POKOK
    ELSE
        JAM LEMBUR = WAKTU KERJA MINGGUAN - BATAS JAM NORMAL
        Upah Mingguan = (Batas Jam Normal * Upah Pokok) + (Jam Lembur * Upah Lembur)

    RETURN UPAH_MINGGUAN
END